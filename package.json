  "version": "1.1.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bin": {
    "numbatd": "./bin/run-server.js"
  },
  "bugs": {
    "url": "https://github.com/ceejbot/numbat-collector/issues"
  },
  "dependencies": {
    "bistre": "~1.0.1",
    "bole": "~2.0.0",
    "dashdash": "~1.8.0",
    "influx": "~2.0.0",
    "json-stream": "~1.0.0",
    "lodash": "~3.6.0",
    "replify": "^1.2.0"
  },
  "devDependencies": {
    "bistre": "~1.0.0",
    "jshint": "~2.5.6",
    "lab": "~5.2.1",
    "mkdirp": "^0.5.0",
    "must": "^0.12.0",
    "repl-client": "^0.3.0",
    "rimraf": "~2.3.2"
  },
  "homepage": "https://github.com/ceejbot/numbat-collector",
  "keywords": [
    "metrics",
    "influxdb",
    "numbat",
    "monitoring"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceejbot/numbat-collector.git"
  },
  "scripts": {
    "start": "node bin/run-server.js config.js",
    "dev": "NODE_ENV=dev bin/run-server.js test/devconfig.js",
