    "lodash": "^2.4.1",
    "objectstream": "0.0.4",
    "restify": "^2.8.1",
    "riemann": "^0.4.0"
  },
  "devDependencies": {
    "jscs": "^1.2.4",
    "jshint": "^2.5.1",
    "lab": "^3.1.1",
    "must": "^0.11.0"
  },
  "homepage": "https://github.com/ceejbot/numbat-collector",
  "keywords": [
    "metrics",
    "influxdb",
    "riemann",
    "monitoring"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceejbot/numbat-collector.git"
  },
  "scripts": {
    "start": "node bin/run-server.js",
    "dev": "NODE_ENV=dev bin/run-server.js",
    "test-suite": "lab -v -t 70 -c test/*.js",
