{
  "name": "numbat-collector",
  "description": "numbat-powered metrics collector & redirector",
  "version": "1.6.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bin": {
    "numbatd": "./bin/run-server.js"
  },
  "bugs": {
    "url": "https://github.com/ceejbot/numbat-collector/issues"
  },
  "dependencies": {
    "bistre": "~1.0.1",
    "bole": "~2.0.0",
    "influx": "~3.4.0",
    "json-stream": "~1.0.0",
    "lodash": "~3.10.0",
    "numbat-influx": "~0.1.1",
    "ws": "1.0.1",
    "replify": "~1.2.0",
    "request": "~2.67.0"
  },
  "devDependencies": {
    "eslint": "~1.10.3",
    "mkdirp": "~0.5.1",
    "mocha": "~2.4.5",
    "must": "~0.13.1",
    "nyc": "~5.5.0",
    "repl-client": "~0.3.0",
    "rimraf": "~2.5.0",
    "sinon": "~1.17.3"
  },
  "homepage": "https://github.com/ceejbot/numbat-collector",
  "keywords": [
    "graphite",
    "influxdb",
    "metrics",
    "monitoring",
    "numbat"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceejbot/numbat-collector.git"
  },
  "scripts": {
    "dev": "NODE_ENV=dev bin/run-server.js test/configs/devconfig.js",
    "lint": "eslint *.js lib/*.js test/*.js",
    "socket": "NODE_ENV=dev bin/run-server.js test/configs/socket-pretty.js",
    "influx": "NODE_ENV=dev bin/run-server.js test/configs/socket-influx9.js",
    "start": "node bin/run-server.js config.js",
    "tcp": "NODE_ENV=dev bin/run-server.js test/configs/tcp-pretty.js",
    "test": "nyc mocha -t 5000 --check-leaks --ui exports -R spec test/test-*.js",
    "coverage": "nyc check-coverage --lines 75 --functions 60 --branches 70",
    "travis": "npm run lint && nyc mocha -R spec -t 5000 test/test-*.js && npm run coverage",
    "udp": "NODE_ENV=dev bin/run-server.js test/configs/udp-file.js",
    "udp-pretty": "NODE_ENV=dev bin/run-server.js test/configs/udp-pretty.js"
  }
}
